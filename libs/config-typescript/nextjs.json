{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Trinity Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "plugins": [{ "name": "next" }],
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "types": ["node", "next"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
